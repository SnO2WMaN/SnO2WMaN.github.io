{
	"name": "sno2wman_website",
	"version": "1.0.0",
	"description": "Website 2019",
	"author": "SnO2WMaN",
	"private": true,
	"scripts": {
		"dev": "nuxt --hostname 0.0.0.0 --port 3000",
		"build": "nuxt build",
		"start": "nuxt start --hostname 0.0.0.0 --port 3000",
		"generate": "nuxt generate",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"lintfix": "npm run lintfix:eslint && npm run lintfix:stylelint",
		"lint:eslint": "cross-env NODE_ENV=production eslint --ext .js,.vue --ignore-path .gitignore .",
		"lintfix:eslint": "cross-env NODE_ENV=production eslint --fix --ext .js,.vue --ignore-path .gitignore .",
		"lint:stylelint": "cross-env NODE_ENV=production stylelint --ignore-path .gitignore **/*.vue **/*.s?css",
		"lintfix:stylelint": "cross-env NODE_ENV=production stylelint --fix --ignore-path .gitignore **/*.vue **/*.s?css",
		"clean": "cross-env NODE_ENV=production prettier **/*.js **/*.vue --write --ignore-path .gitignore",
		"precommit": "npm run lint:eslint && npm run lint:stylelint"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.12",
		"@fortawesome/free-brands-svg-icons": "^5.6.3",
		"@fortawesome/free-solid-svg-icons": "^5.6.3",
		"@fortawesome/vue-fontawesome": "^0.1.2",
		"@nuxtjs/pwa": "^2.6.0",
		"case-sensitive-paths-webpack-plugin": "^2.1.2",
		"dotenv": "^6.2.0",
		"mathsass": "^0.10.1",
		"minireset.css": "^0.0.3",
		"node-sass": "^4.11.0",
		"nuxt": "^2.0.0",
		"nuxt-sass-resources-loader": "^2.0.5",
		"sass-loader": "^7.1.0",
		"sass-metallic-ratio": "^0.0.3"
	},
	"devDependencies": {
		"autoprefixer": "^9.4.4",
		"babel-eslint": "^10.0.1",
		"browserslist": "^4.3.7",
		"cross-env": "^5.2.0",
		"eslint": "^5.12.0",
		"eslint-loader": "^2.1.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-vue": "^5.1.0",
		"husky": "^1.2.1",
		"nodemon": "^1.11.0",
		"postcss-short": "^5.0.0",
		"postcss-sorting": "^4.0.1",
		"prettier": "^1.15.3",
		"stylelint": "^9.9.0",
		"stylelint-config-prettier": "^4.0.0",
		"stylelint-config-recommended-scss": "^3.2.0",
		"stylelint-scss": "^3.4.3"
	}
}
